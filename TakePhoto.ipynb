    "import cv2 \n",
    "import time\n",
    "\n",
    "def FrameCapture(path=0, save_path=None): \n",
    "    vidObj = cv2.VideoCapture(path) \n",
    "    time.sleep(1)\n",
    "    success, image = vidObj.read()  \n",
    "    cv2.imwrite(save_path, image) \n",
    "\n"
   ]
